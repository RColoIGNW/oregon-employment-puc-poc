{
  "name": "@oregon-state/client",
  "description": "Pandemic Unemployment Assistance Gatsby/React/Typescript Application",
  "version": "0.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IGNW/oregon-employment-puc-poc"
  },
  "bugs": {
    "url": "https://github.com/IGNW/oregon-employment-puc-poc/issues"
  },
  "scripts": {
    "develop": "gatsby develop -o",
    "dev": "npm run develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:watch": "esw -w . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "test": "jest --detectOpenHandles --verbose",
    "test:watch": "jest --watch",
    "serve": "gatsby serve -p 5000",
    "storybook": "start-storybook -p 6006",
    "clean": "gatsby clean",
    "build": "gatsby build && npm run copy:assets",
    "build:storybook": "build-storybook",
    "deploy": "firebase deploy --only hosting",
    "update": "ncu -u",
    "start": "npm run dev",
    "start:ci": "npm run serve",
    "copy:assets": "cp -R ./src/images/ ./public/images/",
    "plop:component": "plop --plopfile ./node_modules/ignw-component-generator/plopfile.ts",
    "precy:open": "npm run build && npm run serve  & wait-on http://localhost:5000",
    "cy:open": "NODE_ENV=production CYPRESS_BASE_URL=http://localhost:5000 cypress open",
    "test:cy": "cypress run",
    "pretest:ci": "export REACT_APP_API_HOST=https://oed-poc-server-ohkmuktm2a-uw.a.run.app",
    "test:e2e": "gatsby run develop & wait-on http://localhost:8000 && NODE_ENV=development CYPRESS_BASE_URL=http://localhost:8000 cypress run",
    "test:ci": "npm run start:ci & wait-on http://localhost:5000 && NODE_ENV=production CYPRESS_BASE_URL=http://localhost:5000 npm run test:cy"
  },
